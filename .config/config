
general {
        colors = true
        interval = 5
}

order += "volume master"
order += "wireless _first_"
order += "battery all"

order += "tztime local"

wireless _first_ {
        format_up = "W: (%quality)"
        format_down = "W: down"
}


battery all {
        format = "%status %percentage %remaining"
}

tztime local {
        format = "%Y-%m-%d %H:%M:%S"
}

volume master {
        format = "♪: %volume"
        format_muted = "♪: muted (%volume)"
}